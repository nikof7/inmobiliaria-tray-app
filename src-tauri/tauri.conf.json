{
  "$schema": "https://raw.githubusercontent.com/nicoodev/tauri-app-schema/main/schema.json",
  "productName": "Inmobiliaria Inbox",
  "version": "0.1.0",
  "identifier": "com.inmobiliaria.inbox",
  "build": {
    "frontendDist": "../src",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null
    },
    "trayIcon": {
      "id": "main-tray",
      "iconPath": "icons/tray-default.png",
      "iconAsTemplate": true,
      "tooltip": "Inmobiliaria Inbox"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15"
    }
  }
}
